EMBED %OleDropTargetDropCodeBeforeParent
[INSTANCES]
WHEN '6'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
LABEL
PROPERTY:END
    !Sample Code Snippets
    !!!Region enumerate clipboard formats
    OMIT('***')
    !enumerate clipboard formats
    kcr_OpenClipboard(SELF.m_hWnd)
    uFormat = kcr_EnumClipboardFormats(0)
    LOOP WHILE uFormat
       nLengthFormatName = kcr_GetClipboardFormatName(uFormat, cName, 63)
       uFormat = kcr_EnumClipboardFormats(uFormat)
    END
    kcr_CloseClipboard()
    !***
    !!!EndRegion

    !!!Region enumerate datasource formats
    OMIT('***')
    !enumerate datasource formats
    pNewString &= NEW(STRING(SIZE(FORMATETC)))
    fmtetc &= ADDRESS(pNewString)
    IF ~fmtetc &= NULL
       kcr_memset(ADDRESS(fmtetc),0,SIZE(FORMATETC))
       IF pDataObject.EnumFormatEtc(DATADIR_GET,ADDRESS(pEnumFormatEtc)) = S_OK
          EnumFormatEtc &= (pEnumFormatEtc)
          hr = EnumFormatEtc.Reset()
          hr = EnumFormatEtc.RemoteNext(1,ADDRESS(fmtetc),ADDRESS(celtFetched))
          LOOP WHILE hr = S_OK
             hr = EnumFormatEtc.RemoteNext(1,ADDRESS(fmtetc),ADDRESS(celtFetched))
          END
       END
       DISPOSE(fmtetc)
       fmtetc &= NULL
    END
    pNewString &= NULL
    !***
    !!!EndRegion

    !!!Region handle MSOffice compound document
    !OMIT('***')
    !handle MSOffice compound document
    IF SELF.m_fAllowDrop = 2
       bDropHandled = FALSE
       pNewString &= NEW(STRING(SIZE(FORMATETC)))
       fmtetc &= ADDRESS(pNewString)
       IF ~fmtetc &= NULL
          kcr_memset(ADDRESS(fmtetc),0,SIZE(FORMATETC))
          fmtetc.cfFormat = SELF.m_cfFileGroupDescriptor
          fmtetc.ptd      = 0
          fmtetc.dwAspect = DVASPECT_CONTENT
          fmtetc.lindex   = -1
          fmtetc.tymed    = TYMED_HGLOBAL

          IF pDataObject.QueryGetData(ADDRESS(fmtetc)) = S_OK
             pNewString &= NEW(STRING(SIZE(STGMEDIUM)))
             stgmed &= ADDRESS(pNewString)
             IF ~stgmed &= NULL
                hr = pDataObject.GetData(ADDRESS(fmtetc), ADDRESS(stgmed))
                IF hr = S_OK
                   pFileGroupDescriptor = kcr_globalLock(stgmed.u.hGlobal)
                   fgd &= (pFileGroupDescriptor)

!                   pfd =  ADDRESS(fgd.fd)
!                   OleDataBuffer = ''
!                   LOOP I = 1 TO fgd.cItems
!                      fd &= ( pfd + ((I-1)*SIZE(FILEDESCRIPTOR)) )
!                      !do processing for each file
!                      !example following handles last file dropped
!                   END
                   pNewString &= NEW(STRING(SIZE(FORMATETC)))
                   contents_fmtetc &= ADDRESS(pNewString)
                   IF ~contents_fmtetc &= NULL
                      kcr_memset(ADDRESS(contents_fmtetc),0,SIZE(FORMATETC))
                      contents_fmtetc.cfFormat = SELF.m_cfFileContents
                      contents_fmtetc.ptd      = 0
                      contents_fmtetc.dwAspect = DVASPECT_CONTENT
                      contents_fmtetc.lindex   = -1
                      contents_fmtetc.tymed    = BOR(TYMED_ISTREAM,TYMED_ISTORAGE)
                      IF pDataObject.QueryGetData(ADDRESS(contents_fmtetc)) = S_OK
                         bDropHandled = TRUE
                         pNewString &= NEW(STRING(SIZE(STGMEDIUM)))
                         contents_stgmed &= ADDRESS(pNewString)
                         IF ~contents_stgmed &= NULL
                            kcr_memset(ADDRESS(contents_stgmed),0,SIZE(STGMEDIUM))
                            contents_fmtetc.lindex   = 0
                            hr = pDataObject.GetData(ADDRESS(contents_fmtetc), ADDRESS(contents_stgmed))
                            IF hr = S_OK
                               OleDataBuffer = fgd.fd.cFileName
                               CASE contents_stgmed.tymed
                                 !handle stream of ascii text (eg a dropped text email attachment)
                                 OF TYMED_ISTREAM
                                    thisStream &= (contents_stgmed.u.pstm)
                                    hr = thisStream.Stat(ADDRESS(thisStatstg), 0)
                                    IF thisStatstg.cbSize.highpart = 0
                                       cb = CHOOSE(thisStatstg.cbSize.lowpart < SIZE(OleDataBuffer)-1,thisStatstg.cbSize.lowpart,SIZE(OleDataBuffer)-1)
                                       hr = thisStream.Read(ADDRESS(OleDataBuffer), cb, cbRead)
                                    END
                                    thisStream &= NULL

                                 !handle MSOffice compound document
                                 OF TYMED_ISTORAGE
                                    thisStorage &= (contents_stgmed.u.pstg)

                                    !save the stream to a local file
                                    cdFilename = fgd.fd.cFileName
                                    IF wcdFilename.Init(cdFilename)
                                       hr = StgCreateStorageEx(wcdFilename.GetWideStr(),  |
                                                          BOR(BOR(BOR(STGM_DIRECT,STGM_CREATE),STGM_READWRITE),STGM_SHARE_EXCLUSIVE), |
                                                          STGFMT_STORAGE,0,0,0,ADDRESS(_ISTORAGE),pObjectOpen)
                                       IF hr = S_OK
                                          myStorage &= (pObjectOpen)
                                          hr = thisStorage.CopyTo(0,0,0,myStorage)
                                          myStorage.Release()
                                       END
                                    END

                                    !enumerate the text elements in the compound document
                                    hr = thisStorage.EnumElements(0,0,0,pEnum)
                                    IF hr = S_OK
                                       FREE(DocumentQ)
                                       thisEnum &= (pEnum)
                                       hr = thisEnum.Next(1,thisStatstg,celtFetched)
                                       LOOP WHILE hr = S_OK
                                          IF thisStatstg.type = STGTY_STREAM
                                             CLEAR(DocumentQ)
                                             hr = szElement.Init(thisStatstg.pwcsName)
                                             DocumentQ.Element = szElement.GetCStr()
                                             szElement.Release()
                                             IF SUB(DocumentQ.Element,-4,4) = '001E'   !If Text Type Element
                                                hr = thisStorage.OpenStream(thisStatstg.pwcsName,0, |
                                                             BOR(BOR(STGM_DIRECT,STGM_READ),STGM_SHARE_EXCLUSIVE), |
                                                             0,pIStream)
                                                IF hr = S_OK
                                                   thisStream &= (pIStream)
                                                   hr = thisStream.Stat(ADDRESS(thisStatstg), 0)
                                                   IF thisStatstg.cbSize.highpart = 0
                                                      cb = CHOOSE(thisStatstg.cbSize.lowpart < SIZE(OleDataBuffer)-1,thisStatstg.cbSize.lowpart,SIZE(OleDataBuffer)-1)
                                                      thisString &= NEW CSTRING(cb+1)
                                                      kcr_memset(ADDRESS(thisString),0,cb+1)
                                                      hr = thisStream.Read(ADDRESS(thisString), cb, cbRead)
                                                      DocumentQ.Value = thisString
                                                      !IF CLIP(DocumentQ.Value)         !only show non-blank elements
                                                         ADD(DocumentQ)
                                                      !END
                                                      DISPOSE(thisString)
                                                      thisString &= NULL
                                                   END
                                                   thisStream.Release()
                                                   thisStream &= NULL
                                                END
                                             ELSE
                                                DocumentQ.Value = ''
                                                ADD(DocumentQ)
                                             END

                                          END
                                          hr = thisEnum.Next(1,thisStatstg,celtFetched)
                                       END
                                    END
                               END
                               IF contents_StgMed.pUnkForRelease
                                  iUnk &= (contents_StgMed.pUnkForRelease)
                                  iUnk.Release()
                                  iUnk &= NULL
                               ELSE
                                  kcr_ReleaseStgMedium(contents_stgmed)
                               END
                               DISPOSE(contents_stgmed)
                               contents_stgmed &= NULL
                               POST(SELF.m_OleDropEvent)
                            ELSE
                               kcr_itoa(hr,hr_hex,16)
                               MESSAGE('GetData(' & fmtetc.lindex & ') returned ' & hr_hex)
                            END
                         END
                      END
                      DISPOSE(contents_fmtetc)
                      contents_fmtetc &= NULL
                   END
                   pNewString &= NULL





                   kcr_globalUnlock(stgmed.u.hGlobal)
                   IF StgMed.pUnkForRelease
                      iUnk &= (StgMed.pUnkForRelease)
                      iUnk.Release()
                      iUnk &= NULL
                   ELSE
                      kcr_ReleaseStgMedium(stgmed)
                   END
                   DISPOSE(stgmed)
                   stgmed &= NULL
                END
             END
          ELSE
            message('FileGroupDescriptor Not Found')
          END
          DISPOSE(fmtetc)
          fmtetc &= NULL
       END
       pNewString &= NULL
    END
    !***
    !!!EndRegion

    !!!Region Handle CF_HDROP (Display Filenames and Count)
    !OMIT('***')
    !handle CF_HDROP
    IF SELF.m_fAllowDrop = 3
       bDropHandled = FALSE
       pNewString &= NEW(STRING(SIZE(FORMATETC)))
       fmtetc &= ADDRESS(pNewString)
       IF ~fmtetc &= NULL
          kcr_memset(ADDRESS(fmtetc),0,SIZE(FORMATETC))
          fmtetc.cfFormat = CF_HDROP
          fmtetc.ptd      = 0
          fmtetc.dwAspect = DVASPECT_CONTENT
          fmtetc.lindex   = -1
          fmtetc.tymed    = TYMED_HGLOBAL
          IF pDataObject.QueryGetData(ADDRESS(fmtetc)) = S_OK
             bDropHandled = TRUE
             pNewString &= NEW(STRING(SIZE(STGMEDIUM)))
             stgmed &= ADDRESS(pNewString)
             IF ~stgmed &= NULL
                kcr_memset(ADDRESS(stgmed),0,SIZE(STGMEDIUM))
                fmtetc.lindex   = 0
                hr = pDataObject.GetData(ADDRESS(fmtetc), ADDRESS(stgmed))
                IF hr = S_OK
                   hDrop = kcr_GlobalLock(stgmed.u.hGlobal)
                   cFiles = kcr_DragQueryFile(hdrop, -1, pszFile, 0)
                   OleDataBuffer = 'There are ' & cFiles & ' files/directories<0DH,0AH>'
                   LOOP count = 0 TO cFiles-1
                      kcr_DragQueryFile(hdrop, count, szFile, SIZE(szFile))
                      OleDataBuffer = OleDataBuffer & szFile & '<0DH,0AH>'
                   END
                   kcr_globalUnlock(stgmed.u.hGlobal)
                   IF StgMed.pUnkForRelease
                      iUnk &= (StgMed.pUnkForRelease)
                      iUnk.Release()
                      iUnk &= NULL
                   ELSE
                      kcr_ReleaseStgMedium(stgmed)
                   END
                END
                DISPOSE(stgmed)
                stgmed &= NULL
                POST(SELF.m_OleDropEvent)
             END
          END
          DISPOSE(fmtetc)
          fmtetc &= NULL
       END
       pNewString &= NULL
       IF bDropHandled
!          RETURN hr            !<<<------Return before default code executes
       END
    END
    !***
    !!!EndRegion

    !!!Region Handle CF_HDROP (Save Last File)
    OMIT('***')
    !handle CF_HDROP
    IF SELF.m_fAllowDrop = 3
    !Get Filenames
       bDropHandled = FALSE
       pNewString &= NEW(STRING(SIZE(FORMATETC)))
       fmtetc &= ADDRESS(pNewString)
       IF ~fmtetc &= NULL
          kcr_memset(ADDRESS(fmtetc),0,SIZE(FORMATETC))
          fmtetc.cfFormat = SELF.m_cfFileGroupDescriptor
          fmtetc.ptd      = 0
          fmtetc.dwAspect = DVASPECT_CONTENT
          fmtetc.lindex   = -1
          fmtetc.tymed    = TYMED_HGLOBAL

          IF pDataObject.QueryGetData(ADDRESS(fmtetc)) = S_OK
             pNewString &= NEW(STRING(SIZE(STGMEDIUM)))
             stgmed &= ADDRESS(pNewString)
             IF ~stgmed &= NULL
                hr = pDataObject.GetData(ADDRESS(fmtetc), ADDRESS(stgmed))
                IF hr = S_OK
                   pFileGroupDescriptor = kcr_globalLock(stgmed.u.hGlobal)
                   fgd &= (pFileGroupDescriptor)
                   pfd =  ADDRESS(fgd.fd)
                   OleDataBuffer = ''
                   LOOP I = 1 TO fgd.cItems
                      fd &= ( pfd + ((I-1)*SIZE(FILEDESCRIPTOR)) )
                      !do processing for each file
                      !example following handles last file dropped
                   END
                   kcr_globalUnlock(stgmed.u.hGlobal)
                   IF StgMed.pUnkForRelease
                      iUnk &= (StgMed.pUnkForRelease)
                      iUnk.Release()
                      iUnk &= NULL
                   ELSE
                      kcr_ReleaseStgMedium(stgmed)
                   END
                   DISPOSE(stgmed)
                   stgmed &= NULL
                END
             END
          ELSE
            message('CF_FileGroupDescriptor missing')
          END
          DISPOSE(fmtetc)
          fmtetc &= NULL
       END
       pNewString &= NULL

       !process the last of the dropped files
       pNewString &= NEW(STRING(SIZE(FORMATETC)))
       fmtetc &= ADDRESS(pNewString)
       IF ~fmtetc &= NULL
          kcr_memset(ADDRESS(fmtetc),0,SIZE(FORMATETC))
          fmtetc.cfFormat = SELF.m_cfFileContents
          fmtetc.ptd      = 0
          fmtetc.dwAspect = DVASPECT_CONTENT
          fmtetc.lindex   = -1
          fmtetc.tymed    = BOR(TYMED_ISTREAM,TYMED_ISTORAGE)
          IF pDataObject.QueryGetData(ADDRESS(fmtetc)) = S_OK
             bDropHandled = TRUE
             pNewString &= NEW(STRING(SIZE(STGMEDIUM)))
             stgmed &= ADDRESS(pNewString)
             IF ~stgmed &= NULL
                kcr_memset(ADDRESS(stgmed),0,SIZE(STGMEDIUM))
                fmtetc.lindex   = 0
                hr = pDataObject.GetData(ADDRESS(fmtetc), ADDRESS(stgmed))
                IF hr = S_OK
                   OleDataBuffer = fgd.fd.cFileName
                   message(fgd.fd.cFileName)
                   CASE stgmed.tymed
                     !handle stream of ascii text (eg a dropped text email attachment)
                     OF TYMED_ISTREAM
                        message('stream')
                        thisStream &= (stgmed.u.pstm)
                        hr = thisStream.Stat(ADDRESS(thisStatstg), 0)
                        IF thisStatstg.cbSize.highpart = 0
                           cb = CHOOSE(thisStatstg.cbSize.lowpart < SIZE(OleDataBuffer)-1,thisStatstg.cbSize.lowpart,SIZE(OleDataBuffer)-1)
                           hr = thisStream.Read(ADDRESS(OleDataBuffer), cb, cbRead)
                        END
                        thisStream &= NULL

                     !handle MSOffice compound document
                     OF TYMED_ISTORAGE
                        message('storage')
                        thisStorage &= (stgmed.u.pstg)

                        !save the stream to a local file
                        cdFilename = fgd.fd.cFileName
                        IF wcdFilename.Init(cdFilename)
                           hr = StgCreateStorageEx(wcdFilename.GetWideStr(),  |
                                              BOR(BOR(BOR(STGM_DIRECT,STGM_CREATE),STGM_READWRITE),STGM_SHARE_EXCLUSIVE), |
                                              STGFMT_STORAGE,0,0,0,ADDRESS(_ISTORAGE),pObjectOpen)
                           IF hr = S_OK
                              myStorage &= (pObjectOpen)
                              hr = thisStorage.CopyTo(0,0,0,myStorage)
                              myStorage.Release()
                           END
                        END

                        !enumerate the text elements in the compound document
                        hr = thisStorage.EnumElements(0,0,0,pEnum)
                        IF hr = S_OK
                           FREE(DocumentQ)
                           thisEnum &= (pEnum)
                           hr = thisEnum.Next(1,thisStatstg,celtFetched)
                           LOOP WHILE hr = S_OK
                              IF thisStatstg.type = STGTY_STREAM
                                 CLEAR(DocumentQ)
                                 hr = szElement.Init(thisStatstg.pwcsName)
                                 DocumentQ.Element = szElement.GetCStr()
                                 szElement.Release()
                                 IF SUB(DocumentQ.Element,-4,4) = '001E'   !If Text Type Element
                                    hr = thisStorage.OpenStream(thisStatstg.pwcsName,0, |
                                                 BOR(BOR(STGM_DIRECT,STGM_READ),STGM_SHARE_EXCLUSIVE), |
                                                 0,pIStream)
                                    IF hr = S_OK
                                       thisStream &= (pIStream)
                                       hr = thisStream.Stat(ADDRESS(thisStatstg), 0)
                                       IF thisStatstg.cbSize.highpart = 0
                                          cb = CHOOSE(thisStatstg.cbSize.lowpart < SIZE(OleDataBuffer)-1,thisStatstg.cbSize.lowpart,SIZE(OleDataBuffer)-1)
                                          thisString &= NEW CSTRING(cb+1)
                                          kcr_memset(ADDRESS(thisString),0,cb+1)
                                          hr = thisStream.Read(ADDRESS(thisString), cb, cbRead)
                                          DocumentQ.Value = thisString
                                          IF CLIP(DocumentQ.Value)         !only show non-blank elements
                                             ADD(DocumentQ)
                                          END
                                          DISPOSE(thisString)
                                          thisString &= NULL
                                       END
                                       thisStream.Release()
                                       thisStream &= NULL
                                    END
                                 END

                              END
                              hr = thisEnum.Next(1,thisStatstg,celtFetched)
                           END
                        END
                   END
                   IF StgMed.pUnkForRelease
                      iUnk &= (StgMed.pUnkForRelease)
                      iUnk.Release()
                      iUnk &= NULL
                   ELSE
                      kcr_ReleaseStgMedium(stgmed)
                   END
                   DISPOSE(stgmed)
                   stgmed &= NULL
                   POST(SELF.m_OleDropEvent)
                ELSE
                   kcr_itoa(hr,hr_hex,16)
                   MESSAGE('GetData(' & fmtetc.lindex & ') returned ' & hr_hex)
                END
             END
          ELSE
             message('CF_FileContents missing')
          END
          DISPOSE(fmtetc)
          fmtetc &= NULL
       END
       pNewString &= NULL
       IF bDropHandled = TRUE
          RETURN hr            !<<<------Return before default code executes
       END
    END
    !***
    !!!EndRegion
[END]
[END]

  LOOP qfield = 1 TO 3
    qchar = 0
    LOOP RANDOM(1,20) TIMES
      qchar += 1
      newch = RANDOM(65,122)
      LOOP WHILE INRANGE(newch,91,96)
        newch = RANDOM(65,122)
      END
      EXECUTE qfield

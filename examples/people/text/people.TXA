   fmtetc.dwAspect  = DVASPECT_CONTENT
   fmtetc.lindex    = -1
   fmtetc.tymed     = TYMED_HGLOBAL
   bResult = CHOOSE(pDataObject.QueryGetData(ADDRESS(fmtetc)) = S_OK,2,FALSE)
!END
IF ~bResult     !no MSOffice ObjectDescriptor, look for CF_HDROP
   fmtetc.cfFormat  = CF_HDROP
   fmtetc.ptd       = 0
   fmtetc.dwAspect  = DVASPECT_CONTENT
   fmtetc.lindex    = -1
   fmtetc.tymed     = TYMED_HGLOBAL
   bResult = CHOOSE(pDataObject.QueryGetData(ADDRESS(fmtetc)) = S_OK,3,FALSE)
END
